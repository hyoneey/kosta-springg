<title>Insert title here</title>
</head>
<body>
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/2.14/jquery.min.js"></script>
	<script src="http://cdnjs.cloudflare.com/ajax/libs/handlebars.js/3.0.1/handlebars.js"></script>
	
	<div id="displayDiv">
	
	</div>
	
	<script>
		var source = "<h1><p> {{name}} </p> <p> {{userid}} </p> <p> {{addr}} </p> </h1>";
		var template = Handlebars.compile(source);
		var data = {name:"홍길동", userid :"user" , addr : "서울" };
		
		$("#displayDiv").html(template(data));
	</script>
	
</body>
